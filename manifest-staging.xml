<?xml version="1.0" encoding="UTF-8"?>
<!--
Production/Staging Manifest for Excel Add-in
This manifest should be used for staging and production deployments.
Update the domain names and GUID before deployment.
-->
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">
  
  <!-- Unique ID for your staging environment (generate a new GUID) -->
  <Id>a1b2c3d4-e5f6-7890-abcd-ef1234567890</Id>
  
  <!-- Version information -->
  <Version>1.0.0.0</Version>
  
  <!-- Provider information -->
  <ProviderName>Your Company Name</ProviderName>
  
  <!-- Default locale -->
  <DefaultLocale>en-US</DefaultLocale>
  
  <!-- Display name -->
  <DisplayName DefaultValue="PrimeExcelence (Staging)"/>
  <Description DefaultValue="Excel Add-in for financial data management - Staging Environment"/>
  
  <!-- Icons -->
  <IconUrl DefaultValue="https://server01.intranet.local:9443/excellence/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://server01.intranet.local:9443/excellence/assets/icon-64.png"/>
  
  <!-- Support and help URLs -->
  <SupportUrl DefaultValue="https://your-company.com/support"/>
  <AppDomains>
    <AppDomain>https://server01.intranet.local:9443</AppDomain>
    <AppDomain>https://your-company.com</AppDomain>
  </AppDomains>
  
  <!-- Hosts -->
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  
  <!-- Default settings -->
  <DefaultSettings>
    <SourceLocation DefaultValue="https://server01.intranet.local:9443/excellence/taskpane.html"/>
  </DefaultSettings>
  
  <!-- Permissions -->
  <Permissions>ReadWriteDocument</Permissions>
  
  <!-- Version overrides for different Office versions -->
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <!-- Function file for custom functions -->
          <FunctionFile resid="Commands.Url"/>
          
          <!-- Extension points -->
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                
                <!-- Task pane button -->
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    
    <!-- Resources -->
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://server01.intranet.local:9443/excellence/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://server01.intranet.local:9443/excellence/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://server01.intranet.local:9443/excellence/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Commands.Url" DefaultValue="https://server01.intranet.local:9443/excellence/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://server01.intranet.local:9443/excellence/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="CommandsGroup.Label" DefaultValue="PrimeExcelence"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Show Taskpane"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Open PrimeExcelence task pane for financial data management"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>