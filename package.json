{
  "name": "exceladdin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:functions && webpack --mode production",
    "build:dev": "npm run build:functions && webpack --mode development",
    "build:prod": "npm run build:functions && webpack --config webpack.prod.config.js --mode production",
    "build:staging": "npm run build:functions && webpack --config webpack.prod.config.js --mode production --env staging",
    "build:functions": "custom-functions-metadata generate src/commands/commands.ts src/commands/functions.json",
    "dev": "webpack serve --mode development",
    "start": "webpack serve --mode development",
    "clean": "rm -rf dist node_modules/.cache",
    "lint": "npx tsc --noEmit",
    "validate": "office-addin-validator manifest.xml",
    "validate:staging": "office-addin-validator manifest-staging.xml",
    "setup": "./setup.sh",
    "cert:install": "office-addin-dev-certs install",
    "cert:verify": "office-addin-dev-certs verify",
    "cert:uninstall": "office-addin-dev-certs uninstall",
    "serve:prod": "npx http-server dist -p 3000 -S -C localhost.pem -K localhost-key.pem",
    "analyze": "webpack-bundle-analyzer dist/",
    "deploy:prepare": "npm run clean && npm install && npm run build:staging",
    "deploy:windows": "powershell -ExecutionPolicy Bypass -File deployment/scripts/deploy-windows.ps1",
    "health:check": "powershell -ExecutionPolicy Bypass -File deployment/monitoring/health-check.ps1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.9.2",
    "@types/office-js": "^1.0.522",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "@microsoft/office-js": "^1.1.110",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "custom-functions-metadata": "^2.1.3",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
