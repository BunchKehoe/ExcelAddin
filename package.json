{
  "name": "exceladdin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "vite build --mode staging",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "dev": "vite",
    "dev:backend": "cd backend && python run.py",
    "start": "vite",
    "clean": "rm -rf dist node_modules/.cache",
    "lint": "npx tsc --noEmit",
    "validate": "office-addin-validator manifest.xml",
    "validate:staging": "office-addin-validator manifest-staging.xml",
    "validate:prod": "office-addin-validator manifest-prod.xml",
    "setup": "./setup.sh",
    "cert:install": "office-addin-dev-certs install",
    "cert:verify": "office-addin-dev-certs verify",
    "cert:uninstall": "office-addin-dev-certs uninstall",
    "cert:fix": "node fix-certificates.js",
    "cert:fix:ps": "powershell -ExecutionPolicy Bypass -File fix-certificates.ps1",
    "serve:prod": "npx http-server dist -p 3000 -S -C localhost.pem -K localhost-key.pem",
    "preview": "vite preview",
    "deploy:prepare": "npm run clean && npm install && npm run build:staging",
    "deploy:prepare:prod": "npm run clean && npm install && npm run build:prod",
    "deploy:windows": "powershell -ExecutionPolicy Bypass -File deployment/scripts/deploy-windows.ps1",
    "health:check": "powershell -ExecutionPolicy Bypass -File deployment/monitoring/health-check.ps1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.9.2",
    "@types/office-js": "^1.0.522",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "@microsoft/office-js": "^1.1.110",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.2.1",
    "custom-functions-metadata": "^2.1.3",
    "serve": "^14.2.4",
    "typescript": "^5.9.2",
    "vite": "^5.1.4",
    "vite-plugin-static-copy": "^1.0.1"
  }
}
