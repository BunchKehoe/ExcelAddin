{
  "name": "exceladdin",
  "version": "1.0.0",
  "description": "Excel Add-in built with Vite, React, and TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development", 
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache",
    "validate": "office-addin-validator manifest.xml",
    "validate:staging": "office-addin-validator manifest-staging.xml", 
    "validate:prod": "office-addin-validator manifest-prod.xml",
    "cert:install": "office-addin-dev-certs install",
    "cert:verify": "office-addin-dev-certs verify",
    "cert:uninstall": "office-addin-dev-certs uninstall",
    "serve:prod": "vite preview --host 0.0.0.0 --port 3000 --https"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1", 
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.9.2",
    "@types/office-js": "^1.0.522",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "@microsoft/office-js": "^1.1.110",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "express": "^4.18.2",
    "node-windows": "^1.0.0-beta.8"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.9.2",
    "vite": "^6.0.5"
  }
}